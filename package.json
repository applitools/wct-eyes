{
  "name": "wct-eyes",
  "version": "1.2.4",
  "description": "WCT plugin that enables support for local browsers via Selenium",
  "keywords": [
    "wct",
    "web-component-tester",
    "plugin",
    "eyes",
    "applitools"
  ],
  "homepage": "https://applitools.com/",
  "author": {
    "name": "Applitools Team",
    "email": "team@applitools.com",
    "url": "http://www.applitools.com"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/applitools/eyes.wct.git"
  },
  "bugs": {
    "url": "https://github.com/applitools/eyes.wct/issues"
  },
  "directories": {
    "lib": "./lib"
  },
  "files": [
    "README.md",
    "LICENSE",
    "CHANGELOG.md",
    "eyes-browser.js",
    "lib/"
  ],
  "main": "lib/plugin.js",
  "dependencies": {
    "@applitools/eyes-common": "~3.8.0",
    "@applitools/eyes-selenium": "~4.23.0",
    "selenium-webdriver": "^4.0.0-alpha.4",
    "express": "^4.17.1",
    "chalk": "^2.4.2",
    "send": "^0.17.1"
  },
  "devDependencies": {
    "eslint": "^6.0.1",
    "eslint-plugin-import": "^2.18.0",
    "eslint-config-airbnb-base": "^13.2.0",
    "eslint-plugin-node": "^9.1.0"
  },
  "scripts": {
    "lint": "eslint . --ext js"
  },
  "wct-plugin": {
    "cli-options": {
      "apiKey": {
        "help": "Applitools API Key which can be found in the Admin Panel on website.",
        "full": "eyes-api-key"
      },
      "serverUrl": {
        "help": "Eyes server URL that will be used during matching screenshots.",
        "full": "eyes-server-url"
      },
      "proxy": {
        "help": "Defines the proxy server that will be used for requests to Applitools services.",
        "full": "eyes-proxy"
      },
      "forceFullPageScreenshot": {
        "help": "Determines wheter we make a screenshot of region or full-page-screenshot with stitching.",
        "full": "force-full-page-screenshot",
        "flag": true
      },
      "hideScrollbars": {
        "help": "Determines wheter we need to hide scroolbars or not.",
        "full": "hide-scrollbars",
        "flag": true
      },
      "verbose": {
        "help": "Show all Eyes SDK logs into common thread",
        "full": "eyes-verbose",
        "flag": true
      },
      "subsuitesWidth": {
        "help": "A width of subsuites in pixels, will be changed after browser is open",
        "full": "eyes-subsuites-width"
      }
    }
  },
  "license": "SEE LICENSE IN LICENSE",
  "engines": {
    "node": ">= 8.9.0"
  }
}
